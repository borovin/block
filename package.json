{
  "name": "@basket/block",
  "description": "UI blocks based on Custom Elements v1 spec.",
  "version": "2.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/borovin/block.git"
  },
  "main": "block.js",
  "homepage": "https://github.com/borovin/block",
  "scripts": {
    "prepush": "if-env PREPUSH=no || npm run test:full",
    "build": "gulp",
    "build:production": "cross-env NODE_ENV=production gulp",
    "build:coverage": "cross-env NODE_ENV=coverage gulp",
    "start": "serve",
    "test": "ava",
    "test:update": "ava -u",
    "test:full": "npm run test:codestyle && rm -rf node_modules && npm install && npm run build:production && npm test",
    "test:coverage": "npm run build:coverage && nyc ava",
    "test:codestyle": "standard",
    "fix": "standard --fix",
    "dist": "npm run build:production && cross-env PREPUSH=no np"
  },
  "author": "a@borovin.com",
  "license": "MIT",
  "dependencies": {
    "keycode": "^2.1.9",
    "morphdom": "^2.2.1",
    "query-string": "^4.3.4"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-preset-babili": "0.0.12",
    "cross-env": "^5.0.0",
    "fs-extra": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-cli": "^1.2.2",
    "gulp-csso": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-wrap": "^0.13.0",
    "html": "^1.0.0",
    "husky": "^0.13.3",
    "if-env": "^1.0.0",
    "lodash": "^4.17.2",
    "nightmare": "^2.10.0",
    "nyc": "^10.3.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "serve": "^5.0.3",
    "standard": "^10.0.2"
  },
  "pre-commit": [
    "lint",
    "cleanup",
    "install",
    "build:production",
    "test"
  ]
}
